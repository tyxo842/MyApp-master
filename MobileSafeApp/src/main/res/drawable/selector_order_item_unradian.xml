<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>

            <stroke android:width="0.1dp" android:color="@color/order_line_color_color_c6c5ca"/><!--边框颜色order_line_color_color_c6c5ca-->
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>

            <stroke android:width="0.1dp" android:color="@color/order_line_color_color_c6c5ca"/><!--边框颜色order_item_stroke_b-->
        </shape>
    </item>

</selector>